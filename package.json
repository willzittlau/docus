{
  "name": "@nuxt-themes/docus",
  "version": "1.12.0",
  "type": "module",
  "main": "./nuxt.config.ts",
  "packageManager": "pnpm@8.5.1",
  "files": [
    "app",
    "assets",
    "components",
    "composables",
    "layouts",
    "plugins",
    "nuxt.schema.ts",
    "app.vue",
    "app.config.ts",
    "nuxt.config.ts",
    "tokens.config.ts"
  ],
  "scripts": {
    "prepare": "nuxi prepare && nuxi prepare .starters/default",
    "dev": "nuxi dev .starters/default",
    "build": "nuxi build .starters/default",
    "generate": "nuxi generate .starters/default",
    "preview": "nuxi preview .starters/default",
    "lint": "eslint .",
    "release": "release-it"
  },
  "dependencies": {
    "@nuxt-themes/elements": "^0.9.4",
    "@nuxt-themes/tokens": "^1.9.1",
    "@nuxt-themes/typography": "^0.11.0",
    "@nuxt/content": "^2.6.0",
    "@nuxthq/studio": "^0.12.1",
    "@vueuse/nuxt": "^10.1.2"
  },
  "devDependencies": {
    "@algolia/client-search": "^4.17.1",
    "@docsearch/css": "^3.4.0",
    "@docsearch/js": "^3.4.0",
    "@nuxt/devtools": "^0.5.5",
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxtjs/algolia": "^1.7.0",
    "@types/node": "^20.1.5",
    "eslint": "^8.41.0",
    "nuxt": "^3.5.1",
    "release-it": "^15.10.3",
    "typescript": "^5.0.4",
    "vite-plugin-inspect": "^0.7.28",
    "vue": "^3.3.4"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "axios": "0.25.0",
        "vite": "*"
      },
      "ignoreMissing": [
        "postcss",
        "postcss-*",
        "webpack",
        "axios",
        "vite"
      ]
    }
  },
  "license": "MIT",
  "release-it": {
    "npm": {
      "access": "public"
    },
    "git": {
      "commitMessage": "chore(release): release v${version}"
    },
    "github": {
      "release": true,
      "releaseName": "v${version}"
    }
  }
}
